        <div id="imagenes"></div>

        <ul class="list-group">
        #foreach($mensaje in $mensajes)
		<li class="list-group-item">
			$mensaje.getCompra().getNumeroCompra()
			$mensaje.isCantidadPresupuestosIndicada()
